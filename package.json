  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "unit-test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "e2e-serve": "npx sirv build --quiet --single --port 3333",
    "e2e-run": "CYPRESS_baseUrl=http://localhost:3333 npx cypress run",
    "e2e-test": "npx start-server-and-test e2e-serve http://localhost:3333 e2e-run",
    "aws-cache-clear": "aws cloudfront create-invalidation --distribution-id=E1UCZ1564QEQI0 --paths '/*'",
    "aws-deploy": "aws s3 sync build s3://ddakdae-client/",
    "aws-proto-deploy": "aws s3 sync build s3://ddakdae-client-proto/ --acl public-read",
    "aws-proto-clear": "aws s3 rm s3://ddakdae-client-proto/ --recursive"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
